<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wcq.brandservice.dao.mysql.BrandDao">

    <resultMap id="brandInfo" type="com.wcq.category.api.web.dto.CategoryDto">
        <result column="name" property="brandName"></result>
    </resultMap>


    <select id="getBrandInfoByid" resultMap="brandInfo">
        select * from tb_brand where id=#{id}
    </select>
</mapper>